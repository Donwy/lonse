<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lonse">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".webViewActivity"></activity>
        <activity android:name=".sideBarActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".slideMenuActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity android:name=".passwordChangeActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity android:name=".setActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity android:name=".accountsActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity android:name=".loginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
        <activity android:name=".listviewActivity"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>
    </application>


</manifest>